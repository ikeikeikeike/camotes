GET     /                                             application.controllers.Pages.requestForm

GET     /request                                      application.controllers.Pages.request
POST    /request                                      application.controllers.Pages.request

GET     /download1                                    application.controllers.Pages.downloadSync
POST    /download1                                    application.controllers.Pages.downloadSync

GET     /download2                                    application.controllers.Pages.downloadAsync
POST    /download2                                    application.controllers.Pages.downloadAsync

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                                 controllers.Assets.versioned(path="/public", file: Asset)
