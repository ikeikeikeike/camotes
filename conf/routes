GET     /                                             application.controllers.Pages.requestForm

GET     /request                                      application.controllers.Pages.request
POST    /request                                      application.controllers.Pages.request

GET     /download                                     application.controllers.Pages.download
POST    /download                                     application.controllers.Pages.download

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                                 controllers.Assets.versioned(path="/public", file: Asset)
